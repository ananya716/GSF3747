
scp /Users/ananyamahapatra/Desktop/archive-GSF3747.run1.tar amahapat@carbonate.uits.iu.edu:/N/project/HundleyLab/GSF3747   # transfer files to Hundley project space

# GSF3747

cd /N/project/HundleyLab        # navigate to project space

mkdir GSF3747                   # make a directory for files

cd GSF3747                      # enter directory

scp GSF3747* amahapat@carbonate.uits.iu.edu:/N/slate/amahapat/GSF3747             # copy files to your own directory

cd /N/slate/amahapat/GSF3747               # navigate back to your directory

gunzip GSF3747*                            # unzip files

# proceed to alignment

# Command for trasnferring file from Desktop to slate directory

# On local terminal, go to Desktop (cd Desktop)

